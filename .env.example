# AI Receptionist Project Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (Required for AI Engine)
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
MYSQL_ROOT_PASSWORD=secure_root_password
MYSQL_DATABASE=ai_receptionist_development
MYSQL_USER=ai_receptionist
MYSQL_PASSWORD=secure_database_password

# Service URLs (for development)
AI_ENGINE_URL=http://localhost:8081
FREESWITCH_URL=http://localhost:8080
BACKEND_URL=http://localhost:3000
FRONTEND_URL=http://localhost:3001

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Rails Configuration
RAILS_ENV=development
SECRET_KEY_BASE=generate_a_secure_secret_key_here

# AI Engine Configuration
MODEL_NAME=gpt-3.5-turbo
SPEECH_MODEL=whisper-1
TTS_MODEL=tts-1
SESSION_TIMEOUT=3600
MAX_RESPONSE_LENGTH=150
RESPONSE_TEMPERATURE=0.7

# FreeSWITCH Configuration
SIP_PORT=5060
RTP_PORT_RANGE=16384-32768

# Logging
LOG_LEVEL=INFO
DEBUG=true

# Security (Change these in production!)
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# External Services (Optional)
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=your_twilio_phone

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn
NEW_RELIC_LICENSE_KEY=your_newrelic_key